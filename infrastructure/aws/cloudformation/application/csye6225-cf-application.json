
{
  "AWSTemplateFormatVersion" : "2010-09-09",
  "Parameters": {
     "keyTag" : {
      "Type" : "String"
    }
  },
  "Resources" : {
	"MyInstance" : {
	  "Type" : "AWS::EC2::Instance",
 	  "Properties" : {
 	  	"ImageId" : "ami-66506c1c",
 	  	"KeyName" : { "Ref" : "keyTag" },
     	"InstanceType" : "t2.micro",
     	"DisableApiTermination" : "true",
     	 "BlockDeviceMappings": [
          {
          	"DeviceName" : "/dev/sda1",
            "Ebs" : {
              "VolumeType": "gp2",
              "VolumeSize": "16"
            }
          }
        ]

	 }
	}
  }
}

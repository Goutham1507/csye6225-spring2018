{
  "AWSTemplateFormatVersion": "2010-09-09",
  
    "Resources": {
    "myVPC": {
      "Type": "AWS::EC2::VPC",
      "Properties": {
        "CidrBlock": "10.0.0.0/28"
      }
    },



    "myInternetGateway": {
      "Type": "AWS::EC2::InternetGateway"
    },


    "AttachGateway": {
      "Type": "AWS::EC2::VPCGatewayAttachment",
      "Properties": {
        "VpcId": {"Ref": "myVPC"},
        "InternetGatewayId": {
          "Ref": "myInternetGateway"
        }
      }
    },


    "myRouteTable": {
      "Type": "AWS::EC2::RouteTable",
      "Properties": {
        "VpcId": {
          "Ref": "myVPC"
        }
      }
    },

    "myRoute": {
      "Type": "AWS::EC2::Route",
      "DependsOn": "myInternetGateway",
      "Properties": {
        "RouteTableId": {
          "Ref": "myRouteTable"
        },
        "DestinationCidrBlock": "0.0.0.0/0",
        "GatewayId": {
          "Ref": "myInternetGateway"
        }
      }
    }



  }
}